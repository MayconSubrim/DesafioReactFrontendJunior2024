{
    "version": 2,
    "builds": [
      {
        "src": "build",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    },
    "routes" : [
        {
          "src" : "/",
          "dest" : "/"
        },
        {
          "src" : "/completed",
          "dest" : "/"
        },
        {
          "src" : "/todone",
          "dest" : "/"
        }
    ]
  }