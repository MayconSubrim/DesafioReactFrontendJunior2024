{
    "version": 2,
    "builds": [
      {
        "src": "build",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    },
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/"
        }
      ],
    "routes" : [
        {
          "src" : "/",
          "dest" : "/"
        }
    ]
  }